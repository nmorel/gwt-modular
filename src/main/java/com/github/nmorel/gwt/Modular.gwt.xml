<?xml version="1.0" encoding="UTF-8"?>
<module rename-to='modular'>

  <inherits name="com.github.nmorel.gwt.Common" />
  <inherits name="com.github.nmorel.gwt.Home" />
  <inherits name="com.github.nmorel.gwt.View1" />
  <inherits name="com.github.nmorel.gwt.View2" />

  <inherits name="com.google.gwt.logging.Logging" />
  <set-property name="gwt.logging.logLevel" value="INFO" />
  <set-property name="gwt.logging.enabled" value="TRUE" />
  <set-property name="gwt.logging.consoleHandler" value="ENABLED" />

  <entry-point class="com.github.nmorel.gwt.modular.ModularEntryPoint" />

  <source path="modular" />

</module>
